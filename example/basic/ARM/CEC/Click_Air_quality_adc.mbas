module Click_Air_quality_adc

include Click_Air_quality_types

sub procedure airquality_adcInit() 
sub function  airquality_readADC() as uint32_t 

implements

sub procedure airquality_adcInit() 

    ADC1_Init() 
    ADC_Set_Input_Channel(0x01) 

end sub

sub function  airquality_readADC() as uint32_t 
dim
    readADC as uint32_t

    readADC = ADC1_Read(1) 
    result = readADC 

end sub

end.